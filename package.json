{
  "name": "supernova",
  "version": "0.0.1",
  "main": "src/bin/server.js",
  "repository": "git@github.com:black-atom/supernova.git",
  "author": "Black-atom Team",
  "license": "MIT",
  "private": false,
  "scripts": {
    "start": "node src/bin/server.js",
    "start:dev": "nodemon --inspect=0.0.0.0:9229 src/bin/server.js",
    "test": "ava --verbose 'test/**/*.js'",
    "e2e-test": "ava 'test/e2e/**/*.js'",
    "unit-test": "ava 'test/unit/**/*.js'",
    "test-nyc": "nyc ava 'test/**/*.js'",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && ./node_modules/.bin/codecov",
    "lint": "eslint './**/*.js'",
    "migration:undo:dev": "node_modules/.bin/sequelize db:migrate:undo:all --debug true --env test",
    "migration:run:dev": "node_modules/.bin/sequelize db:migrate --debug true --env test"
  },
  "dependencies": {
    "@fnando/cnpj": "^0.1.0",
    "@fnando/cpf": "^0.1.0",
    "bcrypt": "^3.0.0",
    "bluebird": "^3.5.1",
    "body-parser": "1.18.3",
    "cors": "2.8.4",
    "dotenv": "^6.0.0",
    "express": "4.16.3",
    "express-validation": "^1.0.2",
    "joi": "^13.6.0",
    "log4js": "2.11.0",
    "moment": "^2.22.2",
    "morgan": "^1.9.0",
    "pg": "^7.4.3",
    "pg-hstore": "^2.3.2",
    "ramda": "^0.25.0",
    "sequelize": "^4.38.0",
    "sequelize-cli": "^4.0.0"
  },
  "devDependencies": {
    "ava": "^0.25.0",
    "axios": "^0.18.0",
    "babel-eslint": "^9.0.0",
    "codecov": "^3.0.4",
    "eslint": "4.19.1",
    "eslint-config-airbnb-base": "13.0.0",
    "eslint-plugin-ava": "^5.1.0",
    "eslint-plugin-import": "2.12.0",
    "eslint-plugin-jest": "^21.18.0",
    "node-mocks-http": "^1.7.0",
    "nodemon": "^1.18.2",
    "nyc": "^12.0.2",
    "sinon": "^6.2.0",
    "supertest": "^3.1.0"
  }
}
